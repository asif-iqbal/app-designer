<h:html xmlns="http://www.w3.org/2002/xforms" 
  xmlns:h="http://www.w3.org/1999/xhtml" 
  xmlns:ev="http://www.w3.org/2001/xml-events" 
  xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
  xmlns:jr="http://openrosa.org/javarosa" 
  xmlns:orx="http://openrosa.org/xforms/" 
  xmlns:odk="http://www.opendatakit.org/xforms">
 <h:head>
  <h:title>stdManholeInspectionForm</h:title>
  <model>
    <instance>
      <data id="stdManholeInspectionForm">
        <orx:meta>
          <orx:instanceID/>
          <orx:timeEnd/>
          <instanceName/>
          <rowID/>
          <rowETag/>
          <filterType/>
          <filterValue/>
          <formID/>
          <locale/>
          <savepointType/>
          <savepointCreator/>
          <savepointTimestamp/>
        </orx:meta>
        <access_type/>
        <certificate_number/>
        <consumables/>
        <cover_condition/>
        <currently/>
        <equipment/>
        <ft_to_downstream_mh/>
        <geo_info/>
        <grade/>
        <inspection_level/>
        <inspection_status/>
        <loc_aid/>
        <loc_city/>
        <loc_client/>
        <loc_name/>
        <loc_state/>
        <loc_streetAddress/>
        <loc_zip/>
        <marks_to/>
        <mh_point_number/>
        <personnel/>
        <photo_number/>
        <project_arrive_time/>
        <project_depart_time/>
        <project_end_time/>
        <project_start_time/>
        <purpose_of_survey/>
        <ring_condition/>
        <sample_date/>
        <sheet/>
        <sign/>
        <sign_person_name/>
        <skt/>
        <skt_name/>
        <surcharge_from_invert/>
        <vehicle/>
        <weather/>
      </data> 
    </instance> 
    <bind nodeset="/data/meta/instanceID" type="string"/>
    <bind nodeset="/data/meta/timeEnd" type="dateTime"/>
    <bind nodeset="/data/meta/instanceName" type="string"/>
    <bind nodeset="/data/meta/rowID" type="string"/>
    <bind nodeset="/data/meta/rowETag" type="string"/>
    <bind nodeset="/data/meta/filterType" type="string"/>
    <bind nodeset="/data/meta/filterValue" type="string"/>
    <bind nodeset="/data/meta/formID" type="string"/>
    <bind nodeset="/data/meta/locale" type="string"/>
    <bind nodeset="/data/meta/savepointType" type="string"/>
    <bind nodeset="/data/meta/savepointCreator" type="string"/>
    <bind nodeset="/data/meta/savepointTimestamp" type="string"/>
    <bind nodeset="/data/access_type" type="string"/>
    <bind nodeset="/data/certificate_number" type="string"/>
    <bind nodeset="/data/consumables" type="select"/>
    <bind nodeset="/data/cover_condition" type="string"/>
    <bind nodeset="/data/currently" type="decimal"/>
    <bind nodeset="/data/equipment" type="select"/>
    <bind nodeset="/data/ft_to_downstream_mh" type="decimal"/>
    <bind nodeset="/data/geo_info" type="geopoint"/>
    <bind nodeset="/data/grade" type="string"/>
    <bind nodeset="/data/inspection_level" type="string"/>
    <bind nodeset="/data/inspection_status" type="string"/>
    <bind nodeset="/data/loc_aid" type="string"/>
    <bind nodeset="/data/loc_city" type="string"/>
    <bind nodeset="/data/loc_client" type="string"/>
    <bind nodeset="/data/loc_name" type="string"/>
    <bind nodeset="/data/loc_state" type="string"/>
    <bind nodeset="/data/loc_streetAddress" type="string"/>
    <bind nodeset="/data/loc_zip" type="string"/>
    <bind nodeset="/data/marks_to" type="decimal"/>
    <bind nodeset="/data/mh_point_number" type="string"/>
    <bind nodeset="/data/personnel" type="select"/>
    <bind nodeset="/data/photo_number" type="string"/>
    <bind nodeset="/data/project_arrive_time" type="time"/>
    <bind nodeset="/data/project_depart_time" type="time"/>
    <bind nodeset="/data/project_end_time" type="time"/>
    <bind nodeset="/data/project_start_time" type="time"/>
    <bind nodeset="/data/purpose_of_survey" type="string"/>
    <bind nodeset="/data/ring_condition" type="string"/>
    <bind nodeset="/data/sample_date" type="date"/>
    <bind nodeset="/data/sheet" type="decimal"/>
    <bind nodeset="/data/sign" type="binary"/>
    <bind nodeset="/data/sign_person_name" type="string"/>
    <bind nodeset="/data/skt" type="binary"/>
    <bind nodeset="/data/skt_name" type="string"/>
    <bind nodeset="/data/surcharge_from_invert" type="string"/>
    <bind nodeset="/data/vehicle" type="select"/>
    <bind nodeset="/data/weather" type="string"/>
  </model> 
 </h:head> 
 <h:body> 
<upload ref="sign" mediatype="image/*"><label>Dummy</label></upload>
<upload ref="skt" mediatype="image/*"><label>Dummy</label></upload>
 </h:body> 
</h:html>
